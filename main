#include <iostream>
#include <cmath>

using namespace std;

double EarthGravity= 9.8;
double mass;
double accerelation;
int c; 

void Earth()
{
    cout << "Enter mass: "; 
    cin >> mass; 
    accerelation = mass * EarthGravity; 
    cout << accerelation<< " Newtons";
    
}

void choice()
{
    cin >> c; 
    switch (c)
    {
    case 1:
    {
        break;
    }
    case 2:
    {
        break;
    }
    case 3:
    {
        Earth(); 

        break;
    }

    }
}  

int main()

{
    cout << "Mercury can be chosen by: 1"<<endl;
    cout << "Venus can be chosen by: 2" << endl;
    cout << "Mars can be chosen by: 3" << endl;
    cout << "Earth can be chosen by: 4" << endl;
    cout << "Jupiter can be chosen by: 5" << endl;
    cout << "Saturn can be chosen by: 6" << endl;
    cout << "Uranus can be chosen by: 7" << endl;
    cout << "Neptune can be chosen by: 8" << endl;
    cout << "Please choose a planet:";  choice();
}
